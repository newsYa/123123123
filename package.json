    "element-ui": "2.13.2",
    "gantt-ying": "^0.2.3",
    "github-markdown-css": "^5.1.0",
    "gojs": "^2.2.9",
    "js-cookie": "2.2.0",
    "jsonlint": "^1.6.3",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "photo-sphere-viewer": "^4.7.0",
